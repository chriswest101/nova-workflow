{
    "name": "cammac/nova-workflow",
    "description": "A Laravel Nova resource tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "winzou/state-machine": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Cammac\\Workflow\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/chriswest101/state-machine"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Cammac\\Workflow\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
